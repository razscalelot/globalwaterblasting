<!-- Services Banner- [E] -->

<!-- Our Work [S]. -->
<section class="w-full bg-white overflow-hidden py-10 sm:py-12 md:py-16 lg:py-20 xl:py-24">
    <div class="wrapper">
        <div class="flex flex-wrap justify-center -mx-5">
            <div class="w-full md:w-7/12 px-5">
                <p class="text-2xl sm:text-28 md:text-3xl lg:text-4xl xl:text-40 text-primary font-semibold"
                    data-aos="fade-right" data-aos-duration="1500">Would You Like a Quote for Your House Cleaning?</p>
                <h4 class="font-medium opacity-70 mt-3" data-aos="fade-right" data-aos-duration="1500">Fill out the form
                    right to receive your quote</h4>
            </div>
            <div class="w-full md:w-7/12 px-5 mt-5">
                <form class="flex flex-wrap -mx-1 md:-m-3" action="/contactus" method="post">
                    <div class="w-full sm:w-1/2 pt-3 sm:p-2 md:p-3" data-aos="fade-right" data-aos-duration="500">
                        <input id="y_name" type="text" name="name"
                            class="w-full outline-none rounded-lg bg-[#F2F4F7] placeholder:font-semibold p-2.5 sm:p-3 lg:p-4"
                            placeholder="Your Name">
                    </div>
                    <div class="w-full sm:w-1/2 pt-3 sm:p-2 md:p-3" data-aos="fade-right" data-aos-duration="700">
                        <input id="email" type="email" name="email"
                            class="w-full outline-none rounded-lg bg-[#F2F4F7] placeholder:font-semibold p-2.5 sm:p-3 lg:p-4"
                            placeholder="Email">
                    </div>
                    <div class="w-full sm:w-1/2 pt-3 sm:p-2 md:p-3" data-aos="fade-left" data-aos-duration="700">
                        <input id="phone_number" type="tel" name="mobile"
                            class="w-full outline-none rounded-lg bg-[#F2F4F7] placeholder:font-semibold p-2.5 sm:p-3 lg:p-4"
                            placeholder="Phone Number">
                    </div>
                    <div class="w-full sm:w-1/2 pt-3 sm:p-2 md:p-3" data-aos="fade-right" data-aos-duration="1000">
                        <input id="address" type="text" name="address"
                            class="w-full outline-none rounded-lg bg-[#F2F4F7] placeholder:font-semibold p-2.5 sm:p-3 lg:p-4"
                            placeholder="Address">
                    </div>
                    <div class="w-full pt-3 sm:p-2 md:p-3" data-aos="fade-left" data-aos-duration="1000">
                        <input id="post_code" type="tel" name="postcode"
                            class="w-full outline-none rounded-lg bg-[#F2F4F7] placeholder:font-semibold p-2.5 sm:p-3 lg:p-4"
                            placeholder="Postcode">
                    </div>
                    <div class="w-full pt-3 sm:p-2 md:p-3" data-aos="fade-right" data-aos-duration="1300">
                        <textarea name="message" id="massage"
                            class="w-full outline-none rounded-lg bg-[#F2F4F7] placeholder:font-semibold p-2.5 sm:p-3 lg:p-4"
                            placeholder="Massage" rows="3"></textarea>
                    </div>
                    <div class="w-full pt-3 sm:p-2 md:p-3" data-aos="zoom-in" data-aos-duration="1000">
                        <button type="submit"
                            class="block text-sm md:text-base btn-primary w-full text-center cursor-pointer mx-auto">Send</button>
                    </div>
                </form>
            </div>
        </div>
    </div>
</section>
<!-- Our Work [E]. -->
<% if (typeof message !='undefined' ) { %>
    <% if (IsSuccess==false) {%>
        <div id="denideMassage" class="w-full fixed right-0 top-0 anim z-[999]">
            <div class="bg-red-100 border border-red-400 text-red-700 px-4 py-3 rounded relative" role="alert">
                <strong class="font-bold">Please enter valid input!</strong>
                <span class="block sm:inline">
                    <%= message %>
                </span>
                <button class="absolute top-0 bottom-0 right-0 px-4 py-3"
                    onclick="addActive('denideMassage','-translate-y-full')">
                    <svg class="fill-current h-6 w-6 text-red-500" role="button" xmlns="http://www.w3.org/2000/svg"
                        viewBox="0 0 20 20">
                        <title>Close</title>
                        <path
                            d="M14.348 14.849a1.2 1.2 0 0 1-1.697 0L10 11.819l-2.651 3.029a1.2 1.2 0 1 1-1.697-1.697l2.758-3.15-2.759-3.152a1.2 1.2 0 1 1 1.697-1.697L10 8.183l2.651-3.031a1.2 1.2 0 1 1 1.697 1.697l-2.758 3.152 2.758 3.15a1.2 1.2 0 0 1 0 1.698z" />
                    </svg>
                </button>
            </div>
        </div>
        <% } else { %>
            <div id="successMassage" class="w-full fixed right-0 top-0 anim z-[999]">
                <div class="bg-green-100 border border-green-400 text-green-700 px-4 py-3 rounded relative"
                    role="alert">
                    <strong class="font-bold">Thank you!</strong>
                    <span class="block sm:inline">
                        <%= message %>
                    </span>
                    <button class="absolute top-0 bottom-0 right-0 px-4 py-3"
                        onclick="addActive('successMassage','-translate-y-full')">
                        <svg class="fill-current h-6 w-6 text-green-500" role="button"
                            xmlns="http://www.w3.org/2000/svg" viewBox="0 0 20 20">
                            <title>Close</title>
                            <path
                                d="M14.348 14.849a1.2 1.2 0 0 1-1.697 0L10 11.819l-2.651 3.029a1.2 1.2 0 1 1-1.697-1.697l2.758-3.15-2.759-3.152a1.2 1.2 0 1 1 1.697-1.697L10 8.183l2.651-3.031a1.2 1.2 0 1 1 1.697 1.697l-2.758 3.152 2.758 3.15a1.2 1.2 0 0 1 0 1.698z" />
                        </svg>
                    </button>
                </div>
            </div>
            <% } %>
                <% } %>